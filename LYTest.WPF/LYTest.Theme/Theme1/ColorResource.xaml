<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Color x:Key="主色" A="255" R="238" G="238" B="242" />

    
    <SolidColorBrush x:Key="BackgroundColor" Color="#FFF5F7FA" />
    <SolidColorBrush x:Key="CoverColor" Color="#33FFFFFF" />
    
    <SolidColorBrush x:Key="PrimaryColor" Color="#FF409EFF" />
    <SolidColorBrush x:Key="SuccessColor" Color="#FF67C23A" />
    <SolidColorBrush x:Key="WarningColor" Color="#FFE6A23C" />
    <SolidColorBrush x:Key="DangerColor" Color="#FFF56C6C" />
    <SolidColorBrush x:Key="InfoColor" Color="#FF909399" />
    
    <SolidColorBrush x:Key="BorderBaseColor" Color="#FFDCDFE6" />
    
    
   

    <SolidColorBrush x:Key="窗口背景色" Color="{DynamicResource 主色}"/>

    <SolidColorBrush x:Key="窗口背景深色" Color="#FFFFFFFF"/>
    <SolidColorBrush x:Key="分隔条颜色" Color="#FFAAAAAA"/>
    <SolidColorBrush x:Key="边框颜色浅" Color="#FF0099FF"/>

    <SolidColorBrush x:Key="边框颜色" Color="#FFE4E7ED"/>
    <SolidColorBrush x:Key="字体颜色浅" Color="#FF33AA33"/>
    <SolidColorBrush x:Key="字体颜色标准" Color="#FF101010"/>
    <SolidColorBrush x:Key="屏幕颜色" Color="#FF666666"/>
    <SolidColorBrush x:Key="表行颜色" Color="{DynamicResource 主色}"/>
    <SolidColorBrush x:Key="表行颜色1" Color="#FFABCDEF"/>
    <SolidColorBrush x:Key="工作状态颜色" Color="#FF0099FF"/>
    
    <LinearGradientBrush x:Key="线性渐变颜色" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFD9E2EB" Offset="0"/>
        <GradientStop Color="#33FFFFFF" Offset="1"/>
    </LinearGradientBrush>
    
    <SolidColorBrush x:Key="控件有效" Color="#FF006CBE"/>
    <SolidColorBrush x:Key="窗口背景深色半透明" Color="#FF0099FF" Opacity="0.6"/>

    <SolidColorBrush x:Key="网格选中项颜色" Color="#FFCCE8FF" />
    <SolidColorBrush x:Key="网格列头" Color="#FFE2E2E2" />
    <SolidColorBrush x:Key="网格列头颜色" Color="#FF303133" />
    <!--<SolidColorBrush x:Key="网格列头" Color="#FFFFFFFF" />-->
    
</ResourceDictionary>