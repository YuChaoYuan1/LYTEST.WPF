<basemodel1:DockControlDisposable 
              xmlns:basemodel1="clr-namespace:LYTest.WPF.Model"
             x:Class="LYTest.WPF.View.View_SetUpAll"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:LYTest.WPF.View"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800"       
             Background="{DynamicResource 窗口背景深色}"
            Foreground="{DynamicResource 字体颜色标准}" >
    <ScrollViewer HorizontalScrollBarVisibility="Auto"
                  VerticalScrollBarVisibility="Disabled">
        <StackPanel HorizontalAlignment="Stretch"
               Margin="15"
                    Orientation="Horizontal"
               Name="panelMenu"
               VerticalAlignment="Center"/>
    </ScrollViewer>
</basemodel1:DockControlDisposable >
