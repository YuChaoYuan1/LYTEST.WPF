                            <TextBlock Grid.Row="2"   Grid.Column="1" Foreground="Green"   Text="{Binding  Ub, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="2"   Grid.Column="2" Foreground="Green"   Text="{Binding  Ib, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="2"   Grid.Column="3" Foreground="Green"   Text="{Binding  Pb, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="2"   Grid.Column="4" Foreground="Green"   Text="{Binding  Qb, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="2"   Grid.Column="5" Foreground="Green"   Text="{Binding PhaseB, StringFormat=\{0:F4\}}"/>

                            <TextBlock Grid.Row="3"   Grid.Column="1"  Foreground="red"  Text="{Binding Uc, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="3"   Grid.Column="2"  Foreground="red"  Text="{Binding Ic, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="3"   Grid.Column="3"  Foreground="red"  Text="{Binding Pc, StringFormat=\{0:F4\}}"/>
