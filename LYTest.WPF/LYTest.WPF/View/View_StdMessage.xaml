<basemodel:DockControlDisposable  xmlns:basemodel="clr-namespace:LYTest.WPF.Model"
                                  x:Class="LYTest.WPF.View.View_StdMessage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:LYTest.WPF.View"
      mc:Ignorable="d" 
      Foreground="{DynamicResource 字体颜色标准}"
      HorizontalAlignment="Left"
      d:DesignHeight="300"
      d:DesignWidth="600" >

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="55*"/>
            <RowDefinition Height="191*"/>
        </Grid.RowDefinitions>
        <Viewbox Stretch="Uniform" Margin="0,0,-0.2,-0.4" Grid.RowSpan="2">
            <Grid  Width="350" VerticalAlignment="Top" >
                <StackPanel HorizontalAlignment="Left" VerticalAlignment="Top" Grid.Column="1" >

                    <Grid  Height="120" Width="350"   HorizontalAlignment="Left"  Visibility="{Binding IsSingle, Converter={StaticResource BoolToVisibilityConverter}}">

                        <Grid.RowDefinitions>
                            <RowDefinition Height="2*"/>
                            <RowDefinition Height="*"/>
                        </Grid.RowDefinitions>

                        <Grid Grid.Row="0">
                            <Grid.Resources>
                                <Style TargetType="Border">
                                    <Setter Property="BorderBrush" Value="#AFD0FF"/>
                                    <Setter Property="BorderThickness" Value="1"/>
                                </Style>
                                <Style TargetType="TextBlock">
                                    <Setter Property="Foreground" Value="#2E2EFE"/>
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Setter Property="FontSize" Value="15"/>
                                </Style>
                            </Grid.Resources>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="2*"/>
                            </Grid.ColumnDefinitions>
                            <Grid.RowDefinitions>
                                <RowDefinition/>
                                <RowDefinition/>
                            </Grid.RowDefinitions>

                            <Border Grid.Row="0" Grid.Column="0" Grid.RowSpan="2" Grid.ColumnSpan="1"/>
                            <Border Grid.Row="0" Grid.Column="1" Grid.RowSpan="2" Grid.ColumnSpan="1"/>
                            <Border Grid.Row="0" Grid.Column="2" Grid.RowSpan="2" Grid.ColumnSpan="1"/>
                            <Border Grid.Row="0" Grid.Column="3" Grid.RowSpan="2" Grid.ColumnSpan="1"/>
                            <Border Grid.Row="1" Grid.Column="0" Grid.RowSpan="1" Grid.ColumnSpan="4"/>
                            <TextBlock Text="U (V)" Grid.Row="0" Grid.Column="0" FontSize="15" />
                            <TextBlock Text="I (A)" Grid.Row="0" Grid.Column="2" FontSize="15"/>
                            <TextBlock Text="F (Hz)" Grid.Row="1" Grid.Column="0"/>

                            <TextBlock Grid.Row="1"  Grid.Column="2" >
                                <Run Text="Φ"/><Run FontSize="9" Text="UI" BaselineAlignment="Bottom"/>(°)
                            </TextBlock>

                            <TextBlock Grid.Row="0"   Grid.Column="1"   Text="{Binding Ua, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="0"   Grid.Column="3"   Text="{Binding Ia, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="1"   Grid.Column="1"   Text="{Binding Freq, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Row="1"   Grid.Column="3"   Text="{Binding PhaseA, StringFormat=\{0:F4\}}"/>
                        </Grid>

                        <Grid Grid.Row="1" >
                            <Grid.Resources>
                                <Style TargetType="TextBlock">
                                    <Setter Property="Foreground" Value="Black" />
                                    <Setter Property="HorizontalAlignment" Value="Center"/>
                                    <Setter Property="VerticalAlignment" Value="Center"/>
                                    <Setter Property="FontSize" Value="12"/>
                                </Style>
                            </Grid.Resources>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="3.2*"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="3.2*"/>
                                <ColumnDefinition Width="1*"/>
                                <ColumnDefinition Width="3.2*"/>
                            </Grid.ColumnDefinitions>
                            <!--<Border Grid.Column="0" Grid.RowSpan="1" Grid.ColumnSpan="1" BorderBrush="Chocolate" BorderThickness="1"/>
                            <Border Grid.Column="1" Grid.RowSpan="1" Grid.ColumnSpan="1" BorderBrush="Chocolate" BorderThickness="1"/>
                            <Border Grid.Column="2" Grid.RowSpan="1" Grid.ColumnSpan="1" BorderBrush="Chocolate" BorderThickness="1"/>
                            <Border Grid.Column="3" Grid.RowSpan="1" Grid.ColumnSpan="1" BorderBrush="Chocolate" BorderThickness="1"/>
                            <Border Grid.Column="4" Grid.RowSpan="1" Grid.ColumnSpan="1" BorderBrush="Chocolate" BorderThickness="1"/>
                            <Border Grid.Column="5" Grid.RowSpan="1" Grid.ColumnSpan="1" BorderBrush="Chocolate" BorderThickness="1"/>-->

                            <TextBlock Text="P" Grid.Column="0"  FontSize="20" />
                            <TextBlock Text="Q" Grid.Column="2"  FontSize="20" />
                            <TextBlock Text="S" Grid.Column="4"  FontSize="20" />
                            <TextBlock Grid.Column="1" HorizontalAlignment="Left"  FontSize="17"   Text="{Binding  P, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Column="3"  HorizontalAlignment="Left"   FontSize="17"   Text="{Binding  Q, StringFormat=\{0:F4\}}"/>
                            <TextBlock Grid.Column="5"  HorizontalAlignment="Left"   FontSize="17"   Text="{Binding  S, StringFormat=\{0:F4\}}"/>
                        </Grid>
                    </Grid>



                    <Grid Visibility="{Binding IsThree, Converter={StaticResource BoolToVisibilityConverter}}"
